	"include": [
		"**/*.ts",
		"**/*.tsx",
		"../common/**/*.ts",
		"../common/**/*.tsx",
		"../server/**/*.ts",
		"../server/**/*.tsx",
	],
	"compilerOptions": {
		"noEmit": false,
		"target": "ES2017",
